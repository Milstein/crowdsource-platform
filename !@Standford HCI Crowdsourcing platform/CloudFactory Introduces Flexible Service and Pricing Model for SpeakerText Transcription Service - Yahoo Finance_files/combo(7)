
/* bank-rate.js 85077a1 */ YUI.add("media-bank-rate",function(b){function a(c){this.setDefault(c);this.loadIFrame()}a.prototype={setDefault:function(c){this.modId=c.mod_id;this.iframeSrc=c.iframe_src;this.iframeClass=c.iframe_class},loadIFrame:function(){if(!this.iframeClass||!this.iframeSrc){return}this.iframe=b.Node.one("#"+this.modId+" .bd iframe");if(this.iframe){if(this.iframe.inViewportRegion()===true){this.iframe.set("src",this.iframeSrc)}else{var c=b.on("scroll",function(d){this.iframe.set("src",this.iframeSrc);c.detach()},window,this)}}window.addEventListener("message",this.adjustHeight.bind(this),false)},adjustHeight:function(d){if(d.origin.indexOf("bankrate.com")>-1){var c=d.data;if(typeof(c)=="number"){this.iframe.setStyle("height",d.data+"px")}}}};b.namespace("Media").BankRate=a},"0.0.1",{requires:["node"]});